<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <TextView 
        android:id="@+id/text"
    android:layout_width="match_parent"
    android:layout_height="50dp"/>
    
<Button
    android:id="@+id/create1"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:text="createNotification"
    android:onClick="createNotification1" />
<Button
    android:id="@+id/create2"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:text="createNotification"
    android:onClick="createNotification2" />
<Button 
    android:id="@+id/update"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:text="updateNotification"
    android:onClick="updateNotification" />
<Button 
    android:id="@+id/download"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:text="createNotification"
    android:onClick="download"
    />
</LinearLayout>
